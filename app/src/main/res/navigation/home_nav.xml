<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav"
    app:startDestination="@id/home2">

    <fragment
        android:id="@+id/home2"
        android:name="com.example.orderapp.view.home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_serve"
            app:destination="@id/serve" />
        <action
            android:id="@+id/action_home2_to_hisOrder"
            app:destination="@id/hisOrder" />
        <action
            android:id="@+id/action_home2_to_account"
            app:destination="@id/account" />
    </fragment>
    <fragment
        android:id="@+id/serve"
        android:name="com.example.orderapp.view.Serve"
        android:label="fragment_serve"
        tools:layout="@layout/fragment_serve" >
        <action
            android:id="@+id/action_serve_to_home2"
            app:destination="@id/home2" />
        <action
            android:id="@+id/action_serve_to_account"
            app:destination="@id/account" />
    </fragment>
    <fragment
        android:id="@+id/hisOrder"
        android:name="com.example.orderapp.hisOrder"
        android:label="fragment_his_order"
        tools:layout="@layout/fragment_his_order" >
        <action
            android:id="@+id/action_hisOrder_to_home2"
            app:destination="@id/home2" />
    </fragment>
    <fragment
        android:id="@+id/account"
        android:name="com.example.orderapp.account"
        android:label="fragment_account"
        tools:layout="@layout/fragment_account" >
        <action
            android:id="@+id/action_account_to_home2"
            app:destination="@id/home2" />
    </fragment>
</navigation>